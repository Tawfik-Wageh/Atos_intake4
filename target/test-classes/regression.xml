<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Regression Test Suite" verbose="1">
    <test name="All E2E Tests">
        <groups>
            <run>
                <include name="regression"/>
                <include name="smoke"/>
                <include name="browse"/>
                <include name="cart"/>
                <include name="checkout"/>
                <include name="contact"/>
                <include name="logout"/>
                <include name="e2e"/>
            </run>
        </groups>
        <classes>
            <class name="E2EAutomationExerciseTest"/>
            <class name="SignInTest"/>
            <class name="SignUpTest"/>
        </classes>
    </test>
</suite>
